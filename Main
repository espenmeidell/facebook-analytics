from pprint import pprint
import os


def read_data():
    dirs = os.walk("/home/espen/Data/facebook-espenmeidell/messages")
    message_paths = []
    for dir in dirs:
        if "message.json" in dir[2]:
            message_paths.append(dir[0] + dir[2][0])


if __name__ == '__main__':
    read_data()